print("введи число")
number = int(input())
if number < 0 or number >100:
    print(" вы привысили диапазон возможных чисел")
if number == 1 or number == 0:
      print("Число не является ни простым, ни составным.")


k = 0
for i in range(2, number // 2+1):
    if (number% i == 0):
        k = k+1
if (k <= 0):
    print("Число простое")
else:
    print("Число не является простым")
